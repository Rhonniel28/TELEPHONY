            />

        <ImageButton
            android:onClick="callbtn3"
            android:layout_marginTop="8dp"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@drawable/call"
            android:layout_marginRight="9dp"/>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

        <TextView
            android:id="@+id/atename"
            android:textColor="#ffffff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="30sp"
            android:layout_marginTop="5dp"
            android:text="@string/fizz"/>

            <TextView
                android:id="@+id/atenum"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:textColor="#FFFEFE"
                android:text="@string/fizznum"/>


        </LinearLayout>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#6FB6EF">

    </View>




    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ImageButton
            android:onClick="msgbtn5"
            android:layout_marginTop="8dp"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@drawable/message"
            android:layout_marginRight="15dp"/>

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginTop="8dp"
            android:layout_marginRight="5dp"
            android:onClick="callbtn5"
            android:src="@drawable/call"/>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <TextView
            android:id="@+id/bunname"
            android:textColor="#ffffff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="30sp"
            android:layout_marginTop="5dp"
            android:text="@string/trynda"/>

        <TextView
            android:id="@+id/bunnum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:textColor="#FFFEFE"
            android:text="@string/tryndanum"/>
        </LinearLayout>



    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#6FB6EF">

    </View>

    </LinearLayout>
